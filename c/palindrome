//input a number and check whether it is palindrome or not
void main()
{
int a,b,c,r=0;
printf("enter a number to check whether it is palindrome or not");
scanf("%d",&a);
while(a!=0)
{
b=a%10;
r=r*10+b;
a=a/10;
}
if(r==a)
{
printf("palindrome");
}
else
{
printf("not palindrome");
}
}
